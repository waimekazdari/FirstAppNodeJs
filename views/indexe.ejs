<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title> let's take a title</title>
  </head>
  <body>
    <h1><%=title%></h1>
    <ul>
      <% for (var i=0 ;i<nav.length;i++){ %>
      <li><a href="<%=nav[i].Link %>"><%=nav[i].Text%></a></li>
      <% } %>
    </ul>
    <div class="form">
      Sign In
      <form name='signInForm'
      action="/auth/signIn"
      method="post">
      User Name : <input type="text" name="userName" id="userName" >
      Password :  <input type="text" name="password" id="password" >
      <input type="submit" value ="Sign In" >
      </form>
    </div>
    <div>
      Sign Up
      <form name="signUpForm"
      action="/Auth/signUp"
      method="post">
      User Name:<input name="userName" id="userName" >
      Password: <input name="password" id="password" >
      <input type="submit" value ="Sign Up" >
      </form>
    </div>
  </body>
</html>
